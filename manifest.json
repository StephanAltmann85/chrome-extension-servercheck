{
  "manifest_version": 2,

  "name": "ServerCheck",
  "default_locale": "en",
  "description": "__MSG_description__",
  "version": "1.0.4",

  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },

  "icons": { "128": "images/icon_128.png" },

  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "activeTab",
    "storage",
    "alarms",
    "<all_urls>",
    "notifications"
  ]
}