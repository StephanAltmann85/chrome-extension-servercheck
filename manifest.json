{
  "manifest_version": 2,

  "name": "ServerCheck",
  "description": "Extension",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "activeTab",
    "storage",
    "alarms"
  ]
}